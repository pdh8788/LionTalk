<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Label?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ImageView id="image" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@initImg.png" />
			</image>
		</ImageView>
		<VBox spacing="10.0">
			<children>
				<Label id="name" text="이름" />
				<Label id="talkName" text="대화명" />
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</VBox>
	</children>
</HBox>
