<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.Label?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ImageView id="image" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@groupImg.png" />
			</image>
		</ImageView>
		<VBox spacing="10.0">
			<children>
				<HBox>
					<children>
						<Label id="name" alignment="TOP_LEFT" prefHeight="27.0" prefWidth="165.0" text="이름" />
						<Label id="time" alignment="TOP_RIGHT" contentDisplay="RIGHT" prefHeight="27.0" prefWidth="128.0" text="시간" textAlignment="RIGHT" textOverrun="CENTER_ELLIPSIS">
                     <font>
                        <Font size="10.0" />
                     </font></Label>
					</children>
				</HBox>
				<HBox>
					<children>
						<Label id="lastTalk" alignment="TOP_LEFT" lineSpacing="2.0" prefHeight="19.0" prefWidth="230.0" text="마지막 채팅" />
                  <Label fx:id="num" alignment="CENTER" prefHeight="19.0" prefWidth="64.0" text="0" />
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</HBox>
